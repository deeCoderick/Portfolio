            spaceObjects.forEach((obj, index) => {
                const depth = 0.5 + (index * 0.1); // Different depths for different objects
                obj.style.transform = `translateX(${xAxis * depth}px) translateY(${yAxis * depth}px)`;
            });
        });
        
        // Reset transform when mouse leaves
        heroSection.addEventListener('mouseleave', function() {
            heroContainer.style.transform = 'translateZ(50px) rotateY(0deg) rotateX(0deg)';
            
