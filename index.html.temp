            const targetRect = targetIcon.getBoundingClientRect();

            const floatingIcon = document.createElement('div');
            floatingIcon.className = 'floating-social-icon';
            floatingIcon.innerHTML = originalIcon.innerHTML;
            floatingIcon.style.top = `${originalRect.top}px`;
            floatingIcon.style.left = `${originalRect.left}px`;
            floatingIcon.style.background = window.getComputedStyle(originalIcon).backgroundColor;
            document.body.appendChild(floatingIcon);

            // Hide original icon
            originalIcon.style.opacity = '0';

            // Animate floating icon
            const keyframes = [
                { 
                    top: `${originalRect.top}px`, 
                    left: `${originalRect.left}px`,
                    transform: 'rotate(0deg)',
                    offset: 0 
                },
                { 
                    top: `${(originalRect.top + targetRect.top) / 2 - 20}px`,
                    left: `${(originalRect.left + targetRect.left) / 2}px`,
                    transform: 'rotate(180deg)',
                    offset: 0.5 
                },
                { 
                    top: `${targetRect.top}px`, 
                    left: `${targetRect.left}px`,
                    transform: 'rotate(360deg)',
                    offset: 1 
                }
            ];

            const timing = {
                duration: 800,
                easing: 'cubic-bezier(0.4, 0, 0.2, 1)',
                delay: index * 100
            };

            const animation = floatingIcon.animate(keyframes, timing);

            animation.onfinish = () => {
                floatingIcon.remove();
                targetIcon.style.opacity = '1';
                targetIcon.style.transform = 'translateX(0)';
                resolve();
            };
        });
    }

    async function handleScroll() {
        if (!socialLinksPosition || animationInProgress) return;

        const scrollY = window.scrollY;
        const shouldShowSidebar = scrollY > socialLinksPosition.bottom + 50;

        if (shouldShowSidebar && !sidebarVisible) {
            
            sidebarVisible = true;
            socialSidebar.classList.add('visible');


            }

            
        } else if (!shouldShowSidebar && sidebarVisible) {
            sidebarVisible = false;
            socialSidebar.classList.remove('visible');

            // Reset original icons
            heroSocialLinks.querySelectorAll('.hero-social-link').forEach(link => {
                link.style.opacity = '1';
            });

            // Reset sidebar icons
            socialSidebar.querySelectorAll('a').forEach(link => {
                link.style.opacity = '0';
                link.style.transform = 'translateX(50px)';
            });
        }
    }

    // Initialize
    setupSocialSidebar();
    updateSocialLinksPosition();
    handleScroll();

    // Event listeners
    let scrollTimeout;
    window.addEventListener('scroll', () => {
        if (scrollTimeout) clearTimeout(scrollTimeout);
        scrollTimeout = setTimeout(handleScroll, 10);
    }, { passive: true });

    window.addEventListener('resize', () => {
        updateSocialLinksPosition();
        handleScroll();
    });
});
</script>

