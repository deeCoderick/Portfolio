                const rect = profileImage.getBoundingClientRect();
                const imageX = rect.left + rect.width / 2;
                const imageY = rect.top + rect.height / 2;
                
                // Calculate distance from mouse to center of image
                const distX = e.clientX - imageX;
                const distY = e.clientY - imageY;
                
                // Only apply effect if mouse is within a certain range
                const distance = Math.sqrt(distX * distX + distY * distY);
                const maxDistance = 300; // px
                
                if (distance < maxDistance) {
                    // Calculate tilt angle based on mouse position relative to image center
                    // Reduce the effect as mouse gets further from image
                    const factor = 1 - (distance / maxDistance);
                    const tiltX = distY / 25 * factor;
                    const tiltY = -distX / 25 * factor;
                    
                    // Apply transform with tilt and maintain float animation
                    profileImage.style.transform = `rotateX(${tiltX}deg) rotateY(${tiltY}deg)`;
                } else {
                    // Reset transform if mouse is too far
                    profileImage.style.transform = '';
                }
            });
            
            // Reset transform when mouse leaves the window
            document.addEventListener('mouseleave', function() {
                profileImage.style.transform = '';
            });
        });
    </script>

    <!-- Add profile image tilt effect script -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const profileImage = document.getElementById('profile-image');
            const profileContainer = document.querySelector('.hero-profile-image');
            
            if (profileContainer && profileImage) {
                let container = profileContainer.getBoundingClientRect();
                let centerX = container.left + container.width / 2;
                let centerY = container.top + container.height / 2;
                
                document.addEventListener('mousemove', function(e) {
                    const mouseX = e.clientX;
                    const mouseY = e.clientY;
                    
                    // Calculate distance from center
                    const distanceX = mouseX - centerX;
                    const distanceY = mouseY - centerY;
                    const distance = Math.sqrt(distanceX * distanceX + distanceY * distanceY);
                    
                    // Only apply tilt if mouse is within reasonable distance
                    if (distance < 300) {
                        // Calculate tilt angles (max 15 degrees)
                        const tiltX = (distanceY / 300) * 15;
                        const tiltY = -(distanceX / 300) * 15;
                        
                        // Apply transform with transition
                        profileContainer.style.transform = `perspective(1000px) rotateX(${tiltX}deg) rotateY(${tiltY}deg)`;
                    } else {
                        // Reset transform if mouse is too far
                        profileContainer.style.transform = 'perspective(1000px) rotateX(0) rotateY(0)';
                    }
                });
                
                // Reset transform when mouse leaves window
                window.addEventListener('mouseleave', function() {
                    profileContainer.style.transform = 'perspective(1000px) rotateX(0) rotateY(0)';
                });
                
                // Update center position on window resize
                window.addEventListener('resize', function() {
                    container = profileContainer.getBoundingClientRect();
                    centerX = container.left + container.width / 2;
                    centerY = container.top + container.height / 2;
                });
            }
        });
    </script>
