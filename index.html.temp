    <script>
        // Particle effect for hero section
        document.addEventListener('DOMContentLoaded', function() {
            // Create canvas for particles
            const heroSection = document.querySelector('.hero-section');
            if (!heroSection) return;
            
            const canvas = document.createElement('canvas');
            canvas.className = 'particle-canvas';
            canvas.style.position = 'absolute';
            canvas.style.top = '0';
            canvas.style.left = '0';
            canvas.style.width = '100%';
            canvas.style.height = '100%';
            canvas.style.pointerEvents = 'none';
            canvas.style.zIndex = '1';
            canvas.style.opacity = '0.5';
            
            heroSection.appendChild(canvas);
            
            // Set canvas size
            const resizeCanvas = () => {
                canvas.width = heroSection.offsetWidth;
                canvas.height = heroSection.offsetHeight;
            };
            
            window.addEventListener('resize', resizeCanvas);
            resizeCanvas();
            
            // Initialize particles
            const ctx = canvas.getContext('2d');
            const particles = [];
            const particleCount = Math.min(50, window.innerWidth / 20); // Adjust density based on screen width
            
            // Create particles
            for (let i = 0; i < particleCount; i++) {
                particles.push({
                    x: Math.random() * canvas.width,
                    y: Math.random() * canvas.height,
                    radius: Math.random() * 2 + 1,
                    color: `rgba(255, ${Math.floor(Math.random() * 150) + 100}, ${Math.floor(Math.random() * 150) + 100}, ${Math.random() * 0.3 + 0.1})`,
                    velocity: {
                        x: Math.random() * 0.5 - 0.25,
                        y: Math.random() * 0.5 - 0.25
                    }
                });
            }
            
            // Animation loop
            function animate() {
                ctx.clearRect(0, 0, canvas.width, canvas.height);
                
                // Draw and update particles
                particles.forEach(particle => {
                    ctx.beginPath();
                    ctx.arc(particle.x, particle.y, particle.radius, 0, Math.PI * 2);
                    ctx.fillStyle = particle.color;
                    ctx.fill();
                    
                    // Update position
                    particle.x += particle.velocity.x;
                    particle.y += particle.velocity.y;
                    
                    // Wrap around edges
                    if (particle.x < 0) particle.x = canvas.width;
                    if (particle.x > canvas.width) particle.x = 0;
                    if (particle.y < 0) particle.y = canvas.height;
                    if (particle.y > canvas.height) particle.y = 0;
                });
                
                // Continue animation loop
                requestAnimationFrame(animate);
            }
            
            // Start animation
            animate();
            
            // Check theme and adjust particle colors if needed
            function updateParticleColors() {
                const isDark = document.documentElement.getAttribute('data-theme') === 'dark';
                
                particles.forEach(particle => {
                    if (isDark) {
                        // Brighter particles for dark theme
                        particle.color = `rgba(255, ${Math.floor(Math.random() * 150) + 100}, ${Math.floor(Math.random() * 150) + 100}, ${Math.random() * 0.3 + 0.1})`;
                    } else {
                        // Darker particles for light theme
                        particle.color = `rgba(${Math.floor(Math.random() * 100)}, ${Math.floor(Math.random() * 50)}, ${Math.floor(Math.random() * 100)}, ${Math.random() * 0.2 + 0.05})`;
                    }
                });
            }
            
            // Listen for theme changes
            const themeToggle = document.getElementById('manual-theme-toggle');
            if (themeToggle) {
                themeToggle.addEventListener('click', () => {
                    setTimeout(updateParticleColors, 200);
                });
            }
            
            // Initialize based on current theme
            updateParticleColors();
        });
    </script>

    <!-- Theme Toggle and Navigation Scripts -->
    <script>
        // Theme toggle functionality
        document.addEventListener('DOMContentLoaded', function() {
            const themeToggle = document.getElementById('manual-theme-toggle');
            
            if (themeToggle) {
                themeToggle.addEventListener('click', function() {
                    const htmlElement = document.documentElement;
                    const currentTheme = htmlElement.getAttribute('data-theme');
                    const newTheme = currentTheme === 'dark' ? 'light' : 'dark';
                    
                    // Add animation class
                    themeToggle.classList.add('animated');
                    
                    // Remove animation class after animation completes
                    setTimeout(() => {
                        themeToggle.classList.remove('animated');
                    }, 600);
                    
                    // Set the new theme
                    htmlElement.setAttribute('data-theme', newTheme);
                    localStorage.setItem('theme', newTheme);
                    
                    console.log('Theme switched to:', newTheme);
                    
                    // Dispatch a custom event for theme change
                    document.dispatchEvent(new CustomEvent('themeChanged', { 
                        detail: { theme: newTheme } 
                    }));
                    
                    // Fix for Journey section in light mode
                    if (newTheme === 'light') {
                        // Force immediate visibility for all journey items
                        document.querySelectorAll('.journey-item, .journey-content, .journey-date, .journey-tags, .journey-tag').forEach(item => {
                            item.style.opacity = '1';
