    <!-- Final journey section fix script -->
    <script>
        // This script ensures the Journey section is always visible
        // and properly styled regardless of theme or scroll position
        (function() {
            function forceJourneySectionVisibility() {
                // Target all journey-related elements
                var journeyElements = document.querySelectorAll(
                    '#journey, .journey-section, .journey-timeline, ' +
                    '.journey-item, .journey-content, .journey-date, ' +
                    '.journey-marker, .journey-tags, .journey-tag, .timeline-line'
                );
                
                // Force visibility on all elements
                journeyElements.forEach(function(el) {
                    if (!el) return;
                    
                    // Remove any inline styles that might hide elements
                    if (el.style.display === 'none') el.style.removeProperty('display');
                    if (el.style.visibility === 'hidden') el.style.removeProperty('visibility');
                    if (el.style.opacity === '0') el.style.removeProperty('opacity');
                    if (el.style.transform && el.style.transform.includes('scale(0)')) el.style.removeProperty('transform');
                    
                    // Apply appropriate display styles based on element type
                    if (el.classList.contains('journey-item')) {
                        el.style.display = 'flex';
                    } else if (el.classList.contains('journey-tags')) {
                        el.style.display = 'flex';
                    } else if (el.classList.contains('journey-tag')) {
                        el.style.display = 'inline-block';
                    } else {
                        el.style.display = el.tagName.toLowerCase() === 'div' ? 'block' : '';
                    }
                    
                    // Force visibility
                    el.style.visibility = 'visible';
                    el.style.opacity = '1';
                    
                    // Ensure z-index is high enough
                    if (parseInt(window.getComputedStyle(el).zIndex) < 1) {
                        el.style.zIndex = '5';
                    }
                });
                
                // Ensure the timeline line is visible and properly styled
                var timelineLine = document.querySelector('.timeline-line');
                if (timelineLine) {
                    timelineLine.style.position = 'absolute';
                    timelineLine.style.left = '50%';
                    timelineLine.style.transform = 'translateX(-50%)';
                    timelineLine.style.width = '4px';
                    timelineLine.style.height = '100%';
                    timelineLine.style.background = document.documentElement.getAttribute('data-theme') === 'light' 
                        ? 'rgba(58, 86, 228, 0.2)' 
                        : 'rgba(108, 138, 255, 0.2)';
                    timelineLine.style.zIndex = '1';
                }
                
                // Apply responsive styles if needed
                if (window.innerWidth <= 992) {
                    var timelineLine = document.querySelector('.timeline-line');
                    if (timelineLine) {
                        timelineLine.style.left = '15px';
                    }
                    
                    var journeyItems = document.querySelectorAll('.journey-item');
                    journeyItems.forEach(function(item) {
                        item.style.justifyContent = 'flex-start';
                        item.style.marginLeft = '30px';
                    });
                    
                    var journeyContents = document.querySelectorAll('.journey-content');
                    journeyContents.forEach(function(content) {
                        content.style.width = '80%';
                    });
                    
                    var journeyMarkers = document.querySelectorAll('.journey-marker');
                    journeyMarkers.forEach(function(marker) {
                        marker.style.left = '-25px';
                        marker.style.right = 'auto';
                    });
                }
            }
            
            // Run fixes immediately
            forceJourneySectionVisibility();
            
            // Also run after a delay to catch any delayed rendering
            setTimeout(forceJourneySectionVisibility, 500);
            setTimeout(forceJourneySectionVisibility, 1000);
            
            // Run on window resize and scroll
            window.addEventListener('resize', forceJourneySectionVisibility);
            window.addEventListener('scroll', function() {
                // Only run if we're near the journey section to save performance
                var journeySection = document.getElementById('journey');
                if (!journeySection) return;
                
                var rect = journeySection.getBoundingClientRect();
                var windowHeight = window.innerHeight;
                
                if (rect.top <= windowHeight && rect.bottom >= 0) {
                    forceJourneySectionVisibility();
                }
            }, { passive: true });
            
            // Run on theme change
            document.addEventListener('themeChanged', forceJourneySectionVisibility);
        })();
    </script>
    
