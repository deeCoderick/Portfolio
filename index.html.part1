<script>
// Simple social sidebar functionality without animations
document.addEventListener('DOMContentLoaded', function() {
    const heroSocialLinks = document.querySelector('.hero-social-links');
    const socialSidebar = document.querySelector('.social-sidebar');
    
    if (!heroSocialLinks || !socialSidebar) return;
    
    // Clone social links to sidebar
    heroSocialLinks.querySelectorAll('.hero-social-link').forEach(link => {
        const clonedLink = link.cloneNode(true);
        clonedLink.classList.remove('hero-social-link');
        socialSidebar.appendChild(clonedLink);
    });
    
    // Handle scroll with throttling
    let lastScrollTime = 0;
    const throttleDelay = 100; // ms
    
    function updateSidebar() {
        const rect = heroSocialLinks.getBoundingClientRect();
        const shouldShow = window.scrollY > (rect.bottom + window.scrollY + 50);
        socialSidebar.classList.toggle('visible', shouldShow);
    }
    
    // Throttled scroll handler
    window.addEventListener('scroll', () => {
        const now = Date.now();
        if (now - lastScrollTime >= throttleDelay) {
            lastScrollTime = now;
            updateSidebar();
        }
    }, { passive: true });
    
    // Handle resize
    window.addEventListener('resize', updateSidebar);
    
    // Initial check
    updateSidebar();
});
</script>

